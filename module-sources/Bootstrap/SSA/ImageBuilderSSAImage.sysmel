public class ImageBuilderSSAImage superclass: ImageBuilderTargetImage; definition: {
    public field ssaModule => SSAModule.
    
    public override method saveTo: (filename: String) ::=> Boolean := {
        {
            printLine("Writing SSA text IR").
            let ssaTextIR := ssaModule fullPrintString.
            ##printLine(ssaTextIR).
            IO::writeWholeFileNamedWithString(filename -- ".ssa", ssaTextIR)
        }.
        
        true
    }.
}.
