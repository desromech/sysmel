Boolean definition: {
    public final pure eager method not => Boolean := {
        <primitive: #Boolean::not>
        <targetDefinedPrimitive>
        if: self then: false else: true
    }.

    public final pure eager method xor: (other: Boolean) ::=> Boolean := {
        <primitive: #Boolean::xor:>
        <targetDefinedPrimitive>
        if: self == other then: false else: true
    }.
}.
