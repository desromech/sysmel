error adoptDefinitionOf: {:(String)message :: Unwinds |
    "Fatal error: " writeOntoStderr.
    message writeOntoStderr.
    "\n " writeOntoStderr.
    sysmel_pal_abort().
}.

primitiveFailedError adoptDefinitionOf: {:: Unwinds |
    "Fatal error: primitive failed\n" writeOntoStderr.
    sysmel_pal_abort().
}.
